        rdfs:isDefinedBy  crtv: ;
        rdfs:label        "assetTag" ;
        rdfs:seeAlso      <http://open-services.net/wiki/reconciliation/OSLC-Reconciliation-Specification-Version-2.0/#Asset> ;
        owl:sameAs        oslc_asset:tag .

