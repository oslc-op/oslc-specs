            <#severity>,
            <#shortTitle>,
            <#status>,
            <#state>,
            <#subject>,
            <#title>,
            <#tracksChangeSet>,
            <#type>,
            <#authorizer>,
            <#parent>,

            # State predicates
            <#closed>,
            <#inProgress>,
            <#fixed>,
            <#approved>,
            <#reviewed>,
            <#verified>
        .

