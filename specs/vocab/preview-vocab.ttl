<http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/vocab/preview-vocab.ttl>
        rddl:nature  rdfs: .

<http://open-services.net/ns/core#>
        a                    owl:Ontology ;
        rdfs:label           "Core" ;
        rdfs:seeAlso         <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/preview-vocab.ttl>, <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> ;
        dcterms:description  "All vocabulary URIs defined for OSLC resource preview."^^rdf:XMLLiteral ;
        dcterms:source       <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/preview-vocab.ttl> ;
        dcterms:title        "The OSLC Resource Preview Vocabulary" .

<http://open-services.net/ns/core#Compact>
        a                 rdfs:Class ;
        rdfs:comment      "A resource describing how to display a link and Preview for another, associated resource." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "Compact" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> .

<http://open-services.net/ns/core#Preview>
        a                 rdfs:Class ;
        rdfs:comment      "An HTML representation of a resource that can be embedded in another user interface." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "Preview" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> .

<http://open-services.net/ns/core#document>
        rdfs:comment      "The URI of an HTML document to be used for the preview." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "document" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> .

<http://open-services.net/ns/core#hintHeight>
        a                 rdf:Property ;
        rdfs:comment      "Preferred height of a delegated user interface. Values must be expressed using length units as specified in Cascading Style Sheets 2.1." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "hintHeight" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html>,
                            <http://www.w3.org/TR/CSS2/> .

<http://open-services.net/ns/core#hintWidth>
        a                 rdf:Property ;
        rdfs:comment      "Preferred width of a delegated user interface. Values must be expressed using length units as specified in Cascading Style Sheets 2.1." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "hintWidth" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html>,
                            <http://www.w3.org/TR/CSS2/> .

<http://open-services.net/ns/core#icon>
        a                 rdf:Property ;
        rdfs:comment      "URI of an image." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "icon" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> .

<http://open-services.net/ns/core#iconAltLabel>
        a                 rdf:Property ;
        rdfs:comment      "Alternative label used in association with the oslc:icon, such as HTML img tag's alt attribute." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "iconAltLabel" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> .

<http://open-services.net/ns/core#iconTitle>
        a                 rdf:Property ;
        rdfs:comment      "Title used in association with the oslc:icon, such as HTML img tag's title attribute." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "iconTitle" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> .

<http://open-services.net/ns/core#largePreview>
        a                 rdf:Property ;
        rdfs:comment      "URI and sizing properties for an HTML document to be used for a large preview." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "largePreview" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> .

<http://open-services.net/ns/core#shortTitle>
        a                 rdf:Property ;
        rdfs:comment      "Shorter form of dcterms:title for the resource." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "shortTitle" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> .

<http://open-services.net/ns/core#smallPreview>
        a                 rdf:Property ;
        rdfs:comment      "URI and sizing properties for an HTML document to be used for a small preview." ;
        rdfs:isDefinedBy  <http://open-services.net/ns/core#> ;
        rdfs:label        "smallPreview" ;
        rdfs:seeAlso      <http://tools.oasis-open.org/version-control/browse/wsvn/oslc-core/specs/resource-preview.html> .
